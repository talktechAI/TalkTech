{
  "name": "talktech-next-on-pages",
  "private": true,
  "version": "1.2.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && npx wrangler pages deploy .vercel/output/static",
    "preview": "npm run build && npx @cloudflare/next-on-pages@latest && npx wrangler pages dev .vercel/output/static --compatibility-flag nodejs_compat"
  },
  "dependencies": {
    "matter-js": "^0.19.0",
    "next": "^14.2.32",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.15",
    "@types/matter-js": "^0.20.0",
    "@types/react": "19.1.11",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.3",
    "vercel": "^46.1.1",
    "wrangler": "^4.33.1"
  }
}
